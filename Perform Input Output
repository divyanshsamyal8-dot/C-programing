#include<stdio.h>
int main(){
    char ch;
    int a;
    float b; 
    double c;
    printf("Enter chacter : ");
    scanf("%c", &ch);
    printf("Enter Integer Value : ");
    scanf("%d", &a);
    printf("Enter Float Value : ");
    scanf("%f", &b);
    printf("Enter double type value : ");
    scanf("%lf", &c);
    printf("ch=%c a=%d b=%f c=%lf",ch, a, b, c);
    return 0;
}
