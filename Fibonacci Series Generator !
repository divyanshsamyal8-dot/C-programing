#include<stdio.h>
int main(){
    int N, i,  a = 0, b = 1, next; 
    scanf("%d", &N);
       for(i=0;i<N;i++){
           if(i==0){
               printf("%d ", a);
           }
           else if(i==1){
               printf("%d ", b);
           }
           else {
               next = a+b;
           printf("%d ", next);
                a=b;
           b = next;
           }
       }
       
    return 0;
}
