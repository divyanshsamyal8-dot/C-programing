#include<stdio.h>
int main(){
    int R, A, L, W;
    float AC, PC, AS, PS, AR, PR;
    scanf("%d%d%d%d", &R, &A, &L, &W);
    AC = 3.1416 * R * R;
    PC = 3.1416 * 2 * R;
    AS = A * A;
    PS = 4 * A;
    AR = L * W;
    PR = 2*(L+W);
    printf("Circle: %.2f %.2f\nSquare: %.2f %.2f\nRectangle: %.2f %.2f", AC, PC, AS, PS, AR, PR);
    return 0;
}
