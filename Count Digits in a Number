#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N, Count=0;
    scanf("%d", &N);
 if(N==0){
     printf("1");
 }
     else{
    while(N>0){
        N = N/10;
        Count++;   
         
    }  
    printf("%d", Count); 
     }
    
  

    return 0;
}
